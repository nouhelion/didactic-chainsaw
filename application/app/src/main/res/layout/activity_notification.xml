<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@color/white"
    android:layout_height="match_parent"
    tools:context=".notification">

    <TextView
        android:id="@+id/notif"
        android:layout_width="150dp"
        android:layout_height="32dp"
        android:layout_marginTop="20dp"
        android:text="@string/notificatio"
        android:textAppearance="@style/notificatio"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TextContrastCheck" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/not1"
        app:layout_constraintTop_toBottomOf="@+id/notif"
        android:background="@drawable/notnot"
        android:layout_marginTop="70dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_width="320dp"
        android:layout_height="84dp">
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/not2"
        app:layout_constraintTop_toBottomOf="@+id/not1"
        android:background="@drawable/notnot"
        android:layout_marginTop="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_width="320dp"
        android:layout_height="84dp">
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/not3"
        app:layout_constraintTop_toBottomOf="@+id/not2"
        android:background="@drawable/notnot"
        android:layout_marginTop="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_width="320dp"
        android:layout_height="84dp">
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/not4"
        app:layout_constraintTop_toBottomOf="@+id/not3"
        android:background="@drawable/notnot"
        android:layout_marginTop="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_width="320dp"
        android:layout_height="84dp">
    </androidx.constraintlayout.widget.ConstraintLayout>


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/nav"
        android:background="@drawable/nav"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_width="match_parent"
        android:layout_height="50dp">
        <ImageView
            android:id="@+id/hom"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginStart="30dp"
            android:src="@drawable/home_"
            android:layout_width="25dp"
            android:layout_height="40dp"/>
        <ImageView
            android:id="@+id/cal"
            app:layout_constraintStart_toStartOf="@id/hom"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginStart="100dp"
            android:src="@drawable/calender_"
            android:layout_width="25dp"
            android:layout_height="40dp"/>
        <ImageView
            android:id="@+id/not"
            app:layout_constraintStart_toStartOf="@id/cal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginStart="100dp"
            android:src="@drawable/notification"
            android:layout_width="25dp"
            android:layout_height="40dp"/>
        <ImageView
            android:id="@+id/per"
            app:layout_constraintStart_toStartOf="@id/not"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginStart="100dp"
            android:src="@drawable/shape_"
            android:layout_width="25dp"
            android:layout_height="40dp"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>