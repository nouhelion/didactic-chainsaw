<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@color/white"
    android:layout_height="match_parent"
    tools:context=".rendez_vous">

    <EditText
        android:id="@+id/search"
        app:layout_constraintTop_toBottomOf="@+id/rendvous"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="20dp"
        android:layout_width="292dp"
        android:layout_height="25dp"/>
    
    <TextView
        android:id="@+id/rendvous"
        android:layout_width="150dp"
        android:layout_height="32dp"
        android:layout_marginTop="20dp"
        android:text="@string/rendez_vous"
        android:textAppearance="@style/rendez_vous"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TextContrastCheck" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/nav"
        android:background="@drawable/nav"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_width="match_parent"
        android:layout_height="50dp">
        <ImageView
            android:id="@+id/hom"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginStart="30dp"
            android:src="@drawable/home_"
            android:layout_width="25dp"
            android:layout_height="40dp"/>
        <ImageView
            android:id="@+id/cal"
            app:layout_constraintStart_toStartOf="@id/hom"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginStart="100dp"
            android:src="@drawable/calender"
            android:layout_width="25dp"
            android:layout_height="40dp"/>
        <ImageView
            android:id="@+id/not"
            app:layout_constraintStart_toStartOf="@id/cal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginStart="100dp"
            android:src="@drawable/notification_"
            android:layout_width="25dp"
            android:layout_height="40dp"/>
        <ImageView
            android:id="@+id/per"
            app:layout_constraintStart_toStartOf="@id/not"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginStart="100dp"
            android:src="@drawable/shape_"
            android:layout_width="25dp"
            android:layout_height="40dp"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>